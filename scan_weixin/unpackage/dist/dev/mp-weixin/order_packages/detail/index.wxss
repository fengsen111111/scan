.discount_checkbox {
  background: #FFFFFF;
  margin: -10vw auto 0;
  border-radius: 5.33vw;
  width: calc(100% - 10vw);
  height: 8vw;
  display: flex;
  align-items: center;
  padding: 0 5vw;
}
.discountContent {
  padding: 5.33vw 4vw 0;
}
.discountContent > .title {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
  margin-bottom: 5.33vw;
}
.discountContent .item_box {
  padding: 2.67vw;
  background: #FBFBFB;
  border-radius: 1.33vw;
  border: 0.27vw solid #ED1805;
  margin-bottom: 4vw;
}
.discountContent .item_box > .get > .charge {
  width: 21.33vw;
  height: 8vw;
  background: #000000;
  border-radius: 4vw;
  font-weight: 400;
  font-size: 4vw;
  color: #f5e8ae;
}
.discountContent .item_box > .type {
  font-weight: 400;
  font-size: 2.67vw;
  color: #666666;
  margin: 1.33vw 0;
}
.discountContent .item_box > .item {
  padding-bottom: 2.67vw;
  border-bottom: solid 1px #F5F5F5;
}
.discountContent .item_box > .item > .content {
  font-weight: bold;
  font-size: 4vw;
  color: #ED1805;
  flex: auto;
  align-items: flex-end;
}
.discountContent .item_box > .item > .content > text {
  font-weight: 500;
  font-size: 2.67vw;
  color: #666666;
}
.discountContent .item_box > .item > .title {
  font-weight: 400;
  font-size: 3.73vw;
  color: #000000;
  flex: auto;
}
.discountContent .item_box > .item > .title > text {
  font-weight: 500;
  font-size: 2.67vw;
  color: #666666;
}
.discountContent .item_box > .item > image {
  width: 9.6vw;
  height: 9.6vw;
  border-radius: 1.33vw;
  margin-right: 2.67vw;
}
.nav-bar {
  background-color: #ffffff;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
}
.nav-content {
  height: 44px;
  display: flex;
  align-items: center;
  padding: 0 15px;
  border-bottom: 1px solid #eee;
}
.back-btn {
  width: 30px;
  text-align: center;
  font-size: 20px;
}
.title {
  flex: 1;
  text-align: center;
  font-size: 18px;
  font-weight: bold;
}
.payContent {
  padding: 4.93vw 4vw;
}
.payContent > .confirm_two {
  width: 80vw;
  height: 12vw;
  line-height: 12vw;
  text-align: center;
  background: #EFA246;
  border-radius: 6vw;
  margin: 0 auto 0;
  font-weight: bold;
  font-size: 4vw;
  color: #FFFFFF;
}
.payContent > .chooseItem {
  margin-top: 8.27vw;
  padding: 0 6.67vw;
}
.payContent > .chooseItem > .item {
  font-weight: 400;
  font-size: 3.2vw;
  color: #666666;
}
.payContent > .chooseItem > .item > image {
  max-width: 5.33vw;
  max-height: 5.33vw;
}
.payContent > .chooseItem > .item > text {
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
  margin: 0 2.67vw;
}
.payContent > .title {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
}
.container {
  background: #F5F5F5;
}
.container > .bottom {
  width: calc(100% - 2px - 8vw);
  padding: 2.13vw 4vw 0;
  padding-bottom: constant(safe-area-inset-bottom);
  /*兼容 IOS&gt;11.2*/
  padding-bottom: env(safe-area-inset-bottom);
  /*兼容 IOS&gt;11.2*/
  background: #FFFFFF;
  box-shadow: 0 0.4vw 0.8vw 0.13vw rgba(0, 0, 0, 0.16);
  border-radius: 1.33vw 1.33vw 0 0;
}
.container > .bottom > .right {
  align-items: flex-end;
}
.container > .bottom > .right > .pay {
  width: 21.33vw;
  height: 8vw;
  line-height: 8vw;
  text-align: center;
  background: #EFA246;
  border-radius: 6vw;
  font-weight: bold;
  font-size: 4vw;
  color: #FFFFFF;
  margin-left: 2.93vw;
}
.container > .bottom > .right > .add {
  width: 21.33vw;
  height: 8vw;
  line-height: 8vw;
  text-align: center;
  border-radius: 6vw;
  border: 1px solid #EFA246;
  font-weight: bold;
  font-size: 4vw;
  color: #EFA246;
}
.container > .bottom > .left {
  font-weight: bold;
  font-size: 4vw;
  color: #ED1805;
}
.container > .bottom > .left > .price {
  font-weight: 500;
  font-size: 2.67vw;
  color: #000000;
  margin-left: 4.4vw;
}
.container > .content {
  padding: 4vw;
  background: #FFFFFF;
}
.container > .content > .item {
  margin-bottom: 5.07vw;
}
.container > .content > .item > .price {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
}
.container > .content > .item > .price > text {
  font-size: 4.8vw;
  color: #ED1805;
}
.container > .content > .item > .info {
  margin-bottom: 4.67vw;
}
.container > .content > .item > .info > .concat {
  width: 66.67vw;
  font-weight: 400;
  font-size: 3.2vw;
  color: #999999;
}
.container > .content > .item > .info > image {
  width: 21.33vw;
  height: 21.33vw;
  border-radius: 2.67vw;
  margin-right: 4vw;
}
.container > .content > .item > .title {
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
  margin-bottom: 2.67vw;
}
.container > .content > .line {
  height: 1px;
  background: #999999;
  margin-top: 4vw;
  margin-bottom: 2.67vw;
}
.container > .content > .label {
  font-weight: 500;
  font-size: 3.2vw;
  color: #000000;
  margin-bottom: 2.67vw;
}
.container > .content > .order_num {
  font-weight: 500;
  font-size: 3.2vw;
  color: #000000;
  padding-bottom: 2.67vw;
  border-bottom: solid 1px #999999;
  margin-bottom: 5.33vw;
}

