.blank {
  height: 18vw;
}
.cashOutContent {
  padding: 4vw;
}
.cashOutContent > .cash_out {
  width: 80vw;
  height: 12vw;
  line-height: 12vw;
  text-align: center;
  background: #EFA246;
  border-radius: 6vw;
  font-weight: bold;
  font-size: 4vw;
  color: #FFFFFF;
  margin: 15vw auto 0;
}
.cashOutContent > .inputBox {
  height: 8vw;
  background: #F5F5F5;
  border-radius: 1.33vw;
  padding: 0 2.67vw;
}
.cashOutContent > .inputBox > .error {
  flex: 1;
  font-weight: 500;
  font-size: 2.67vw;
  color: #ED1805;
  text-align: right;
}
.cashOutContent > .inputBox > input {
  flex: 1;
}
.cashOutContent > .label {
  font-weight: 500;
  font-size: 4vw;
  color: #000000;
  margin-bottom: 1.2vw;
}
.cashOutContent > .label > .tips {
  font-weight: bold;
  font-size: 3.2vw;
}
.cashOutContent .title {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
  margin-bottom: 4vw;
}
.container {
  height: 100%;
  min-height: 100vh;
  background: #F5F5F5;
}
.container > .item {
  padding: 2.67vw 4vw;
  background: #FFFFFF;
  margin-bottom: 1.33vw;
}
.container > .item > .right {
  width: 20vw;
  font-weight: 500;
  font-size: 2.67vw;
  color: #000000;
}
.container > .item > .right > .bottom {
  margin-top: .8vw;
}
.container > .item > .right > .bottom > text {
  font-weight: bold;
  color: #ED1805;
}
.container > .item > .left > .info {
  font-weight: 500;
  font-size: 2.67vw;
  color: #000000;
  margin-left: 2.67vw;
}
.container > .item > .left > .info .showDetail {
  padding: 1vw;
  background-color: #000000;
  border-radius: 5px;
  font-size: 2vw;
  margin-left: 2vw;
  color: #fee8ae;
}
.container > .item > .left > .info > text {
  font-weight: bold;
  font-size: 3.2vw;
}
.container > .item > .left > image {
  width: 8vw;
  height: 8vw;
  border-radius: 50%;
}
.container > .label {
  font-weight: bold;
  font-size: 3.2vw;
  color: #000000;
  padding: 4vw 4vw 1.33vw;
}
.container > .label > text {
  font-weight: 500;
  font-size: 2.67vw;
  color: #ED1805;
}
.container > .head {
  padding: 0 4vw;
  height: 10.93vw;
  background: #FFFFFF;
}
.container > .head > .right {
  font-weight: bold;
  font-size: 3.2vw;
  color: #000000;
}
.container > .head > .right > .cash_out {
  width: 13.33vw;
  height: 5.33vw;
  line-height: 5.33vw;
  text-align: center;
  background: #EFA246;
  border-radius: 6vw;
  margin-left: 4vw;
  font-weight: bold;
  font-size: 3.2vw;
  color: #FFFFFF;
}
.container > .head > .left {
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
}
.container > .head > .left > text {
  color: #ED1805;
  margin-left: 3.47vw;
}

