.seatContent {
  width: 80vw;
  max-height: 50vh;
  padding: 5.33vw 4vw 0;
  overflow: hidden;
}
.seatContent > .title {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
  margin-bottom: 5.33vw;
}
.seatContent > .confirm {
  width: 80vw;
  height: 12vw;
  line-height: 12vw;
  text-align: center;
  background: #000000;
  border-radius: 6vw;
  margin: 10vw auto 0;
  font-weight: bold;
  font-size: 4vw;
  color: #f5e8ae;
}
.seatContent .list {
  display: grid;
  grid-template-columns: repeat(4, auto);
  grid-column-gap: 10px;
  grid-row-gap: 10px;
}
.seatContent .list > .item {
  border: solid 1px #000000;
  padding: 2px 3vw;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 3vw;
  color: #000000;
  font-weight: bold;
  font-size: 4vw;
}
.seatContent .list > .active {
  background-color: #000000;
  color: #f5e8ae;
}
.payContent {
  padding: 4.93vw 4vw;
}
.payContent > .confirm {
  width: 80vw;
  height: 12vw;
  line-height: 12vw;
  text-align: center;
  background: #000000;
  border-radius: 6vw;
  margin: 10vw auto 0;
  font-weight: bold;
  font-size: 4vw;
  color: #f5e8ae;
}
.payContent > .price {
  font-weight: bold;
  font-size: 4.8vw;
  color: #ED1805;
}
.payContent > .price > .number {
  width: 34.67vw;
  height: 8vw;
  border-radius: 4vw;
  overflow: hidden;
}
.payContent > .price > .number > .min {
  width: 10.67vw;
  height: 100%;
  background: #000000;
}
.payContent > .price > .number > .num {
  width: 13.33vw;
  height: 100%;
  background: #F5F5F5;
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
}
.payContent > .price > .number > .add {
  width: 10.67vw;
  height: 100%;
  background: #000000;
}
.payContent > .content {
  font-weight: bold;
  font-size: 4vw;
  color: #ED1805;
  margin: 4vw 0 9vw;
}
.payContent > .content > .right {
  flex: auto;
  margin-left: 4vw;
}
.payContent > .content > .right > .top {
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
  margin-bottom: 1vw;
}
.payContent > .content > image {
  width: 12vw;
  height: 12vw;
  border-radius: 1.33vw;
}
.payContent > .title {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
}
.container {
  background: #F5F5F5;
}
.container > .bottom {
  width: calc(100% - 2px - 8vw);
  position: fixed;
  bottom: 0;
  padding: 2.13vw 4vw 0;
  padding-bottom: constant(safe-area-inset-bottom);
  /*兼容 IOS&gt;11.2*/
  padding-bottom: env(safe-area-inset-bottom);
  /*兼容 IOS&gt;11.2*/
  background: #FFFFFF;
  box-shadow: 0 0.4vw 0.8vw 0.13vw rgba(0, 0, 0, 0.16);
  border-radius: 1.33vw 1.33vw 0 0;
  border: 1px solid #666666;
}
.container > .bottom > .pay {
  width: 30%;
  height: 8vw;
  line-height: 8vw;
  text-align: center;
  background: #010101;
  border-radius: 6vw;
  font-weight: bold;
  font-size: 4vw;
  color: #DDC285;
  margin-left: 2.93vw;
}
.container > .bottom > .add {
  width: 30%;
  height: 8vw;
  line-height: 8vw;
  text-align: center;
  border-radius: 6vw;
  border: 1px solid #000000;
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
}
.container > .bottom > .jsdd {
  width: 30%;
  height: 8vw;
  line-height: 8vw;
  text-align: center;
  border-radius: 6vw;
  border: 0.27vw solid #ff0000;
  font-weight: bold;
  font-size: 4vw;
  color: #ff0000;
  margin-left: 16rpx;
}
.container > .list {
  margin-top: 1.87vw;
  padding: 2.67vw 4vw;
  background: #FFFFFF;
}
.container > .list > .label {
  font-weight: bold;
  font-size: 3.2vw;
  color: #000000;
  padding-bottom: 2.93vw;
  border-bottom: solid 1px #999999;
  margin-bottom: 4vw;
}
.container > .list > .label > text {
  font-weight: 500;
  font-size: 2.67vw;
  color: #ED1805;
}
.container > .list > .item {
  margin-bottom: 5.07vw;
}
.container > .list > .item > .price {
  font-weight: bold;
  font-size: 4.27vw;
  color: #000000;
}
.container > .list > .item > .price > text {
  font-size: 4.8vw;
  color: #ED1805;
}
.container > .list > .item > .info {
  margin-bottom: 4.67vw;
}
.container > .list > .item > .info > .concat {
  width: 66.67vw;
  font-weight: 400;
  font-size: 3.2vw;
  color: #999999;
}
.container > .list > .item > .info > image {
  width: 21.33vw;
  height: 21.33vw;
  border-radius: 2.67vw;
  margin-right: 4vw;
}
.container > .list > .item > .title {
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
  margin-bottom: 2.67vw;
}
.container > .list > .item > .title > text {
  font-weight: 500;
  font-size: 2.67vw;
  color: #ED1805;
}
.container > .content {
  padding: 4vw;
  background: #FFFFFF;
}
.container > .content > .line {
  height: 1px;
  background: #999999;
  margin-top: 4vw;
  margin-bottom: 2.67vw;
}
.container > .content > .label {
  font-weight: 500;
  font-size: 3.2vw;
  color: #000000;
  margin-bottom: 2.67vw;
}
.container > .content > .order_num {
  font-weight: bold;
  font-size: 4vw;
  color: #000000;
  padding-bottom: 2.67vw;
  border-bottom: solid 1px #999999;
  margin-bottom: 5.33vw;
}
.container > .content > .order_num > text {
  font-size: 3.2vw;
}
